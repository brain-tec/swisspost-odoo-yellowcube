            </field>
        </record>

        <record id="view_tracking_form_ext" model="ir.ui.view">
            <field name="name">view_tracking_form_ext</field>
            <field name="model">stock.tracking</field>
            <field name="inherit_id" ref="stock.view_tracking_form" />
            <field name="arch" type="xml">
                <field name="serial" position="after">
                    <field name="packaging_type_id" />
                </field>
            </field>
        </record>

    </data>

</openerp>
