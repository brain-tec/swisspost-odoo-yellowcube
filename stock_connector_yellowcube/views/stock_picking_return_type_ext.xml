<odoo>
    <record id="stock_picking_return_type_form_ext" model="ir.ui.view">
        <field name="name">stock_picking_return_type_form_ext</field>
        <field name="model">stock.picking.return_type</field>
        <field name="inherit_id" ref="stock_connector.stock_picking_return_type_form"/>
        <field name="arch" type="xml">
            <field name="name" position="after">
                <field name="yc_code"/>
            </field>
        </field>
    </record>
    <record id="stock_picking_return_type_tree_ext" model="ir.ui.view">
        <field name="name">stock_picking_return_type_tree_ext</field>
        <field name="model">stock.picking.return_type</field>
        <field name="inherit_id" ref="stock_connector.stock_picking_return_type_tree"/>
        <field name="arch" type="xml">
            <field name="name" position="after">
                <field name="yc_code"/>
            </field>
        </field>
    </record>
    <record id="stock_picking_return_type_search_ext" model="ir.ui.view">
        <field name="name">stock_picking_return_type_search_ext</field>
        <field name="model">stock.picking.return_type</field>
        <field name="inherit_id" ref="stock_connector.stock_picking_return_type_search"/>
        <field name="arch" type="xml">
            <field name="name" position="before">
                <field name="yc_code"/>
            </field>
        </field>
    </record>
</odoo>