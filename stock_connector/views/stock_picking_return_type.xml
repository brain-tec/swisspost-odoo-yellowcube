<odoo>
    <record id="stock_picking_return_type_form" model="ir.ui.view">
        <field name="name">stock_picking_return_type_form</field>
        <field name="model">stock.picking.return_type</field>
        <field name="arch" type="xml">
            <form string="Return Type">
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="stock_picking_return_type_tree" model="ir.ui.view">
        <field name="name">stock_picking_return_type_tree</field>
        <field name="model">stock.picking.return_type</field>
        <field name="arch" type="xml">
            <tree string="Return Types">
                <field name="name"/>
            </tree>
        </field>
    </record>
    <record id="stock_picking_return_type_search" model="ir.ui.view">
        <field name="name">stock_picking_return_type_search</field>
        <field name="model">stock.picking.return_type</field>
        <field name="arch" type="xml">
            <search string="Return Types">
                <field name="name"/>
            </search>
        </field>
    </record>
    <record id="stock_picking_return_type_action" model="ir.actions.act_window">
        <field name="name">Return Types</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">stock.picking.return_type</field>
        <field name="view_mode">tree,form</field>
    </record>
    <menuitem id="stock_connect_stock_picking_return_type_menu_wh"
        parent="stock.menu_warehouse_config"
        name="Return Types"
        sequence="5"
        action="stock_picking_return_type_action"
    />
</odoo>