              <para style="P5">[[ formatLang(get_sum_account_currency(a)) ]]</para>
            </td>
          </tr>
        </blockTable>
        
        <section>
          <para style="P4">[[ repeatIn(get_partners(a), 'p') ]]</para>
          <!--para style="P4">[[ repeatIn(get_partners(), 'p') ]] [[ setLang(p.lang) ]]</para-->
