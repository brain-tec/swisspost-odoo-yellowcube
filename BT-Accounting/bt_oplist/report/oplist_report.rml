            <tr>
              <td>
                <para style="P2">[[ p['Partner'].replace('zzzzzz','Buchungen ohne Partner') ]]</para>
              </td>
              <td>
                <para style="P5"><u>[[ formatLang(p['SumDebit']) ]]</u></para>
              </td>
              <td>
                <para style="P5"><u>[[ formatLang(p['SumCredit']) ]]</u></para>
              </td>
              <td>
                <para style="P5"><u>[[ formatLang((p['SumDebit'] - p['SumCredit'])) ]]</u></para>
              </td>
              <td>
                <para style="P5"><u>[[ formatLang(p['SumCurrency']) ]]</u></para>
              </td>
            </tr>
          </blockTable>
          <para style="terp_default_2">
            <font color="white"> </font>
          </para>
